    "js": ["content3.min.js"],
    "run_at": "document_idle",
    "all_frames": true
  },
  {
    "matches": ["*://meet.google.com/*"],
    "js": ["content6.min.js"],
    "run_at": "document_idle",
    "all_frames": true
  },
  {
    "matches": ["<all_urls>"],
    "js": ["content4.min.js"],
    "run_at": "document_idle",
    "all_frames": false
  },
  {
    "matches": ["*://chat.google.com/*"],
    "js": ["content7.min.js"],
    "run_at": "document_idle",
    "all_frames": true
  },
  {
    "matches": ["https://*/*"],
    "include_globs":  ["https://www.google.*/*"],
    "js": ["content8.min.js"],
    "run_at": "document_idle",
    "all_frames": true
  },
  {
    "matches": ["<all_urls>"],
    "js": ["content9.min.js"],
    "run_at": "document_start",
    "all_frames": true
  },
  {
    "matches": ["<all_urls>"],
    "js": ["content10.min.js"],
    "run_at": "document_start",
    "all_frames": true
  },
  {	
    "matches": ["<all_urls>"],	
    "js": ["content5.min.js"],	
    "run_at": "document_idle",	
    "all_frames": true	
  }
  ],
  "background": {
    "scripts": ["conf.min.js", "securly.min.js"],
