  "name": "Securly for Chromebooks",
  "short_name": "Securly",
  "version": "2.98.65",
  "description": "Secure and manage your 1:1 Chromebooks with Securly",
  "permissions": ["webRequest", "webRequestBlocking", "identity", "*://*/","file://*", "cookies", "tabs", "browsingData", "identity.email", "geolocation", "idle", "downloads", "webNavigation"],
  "content_scripts": [{
    "matches": ["*://*.youtube.com/*", "*://youtube.com/*", "*://youtube-nocookie.com/*","*://*.youtube-nocookie.com/*", "*://docs.google.com/*","*://*.docs.google.com/*"],
